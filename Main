local BloodMoonTycoon = 961409234
local BreakInStory = 4620170611
local BreakInStoryLobby = 3851622790
local game3 = 1234567

if game.PlaceId == BloodMoonTycoon then
  loadstring(game:HttpGet('https://raw.githubusercontent.com/GamingBoyZ379/PinguHub/PinguHub/BMT%20GUI.lua'))()  
  
elseif game.PlaceId == BreakInStory then
  loadstring(game:HttpGet('https://raw.githubusercontent.com/GamingBoyZ379/PinguHub/PinguHub/Break%20In%20(Story)%20GUI.lua'))()

elseif game.PlaceId == BreakInStoryLobby then
 warn ('You Must Load Into An Actual Round Before Loading Break In (Story) GUI')

elseif game.PlaceId == game3 then
 print ('You Have Loaded Game 3!')

setfflag("AbuseReportScreenshotPercentage", 0)

setfflag("DFFlagAbuseReportScreenshot", "False")
end
